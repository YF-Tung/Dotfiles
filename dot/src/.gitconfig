# This is Git's per-user configuration file.
[user]
	name = Yu-Fan Tung
	email = EMAIL_HERE 
[push]
	default = simple
[core]
	editor = /usr/bin/vim
[alias]
	co = checkout
	cm = commit
	ss = status
	br = branch
        lg1 = log --author-date-order --graph --abbrev-commit --decorate --format=format:'%C(bold blue)%h%C(reset) - %C(bold green)(%ar)%C(reset) %C(white)%s%C(reset) %C(dim white)- %an%C(reset)%C(bold yellow)%d%C(reset)' --all
        lg2 = log --author-date-order --graph --abbrev-commit --decorate --format=format:'%C(bold blue)%h%C(reset) - %C(bold cyan)%aD%C(reset) %C(bold green)(%ar)%C(reset)%C(bold yellow)%d%C(reset)%n''          %C(white)%s%C(reset) %C(dim white)- %an%C(reset)' --all
        lg = !"git lg1"
        df = diff
[pull]
	rebase = true
[rebase]
	autoStash = true
[credential]
	helper = cache
