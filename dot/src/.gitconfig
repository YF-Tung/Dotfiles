# This is Git's per-user configuration file.
[user]
	name = Yu-Fan Tung
	EMAIL_HERE
[push]
	default = PUSH_DEFAULT_HERE
[core]
	editor = /usr/bin/vim
[alias]
	co = checkout
	cm = commit
	ss = status
	br = branch
        lg1 = log --author-date-order --graph --abbrev-commit --decorate --format=format:'%C(bold blue)%h%C(reset) - %C(bold green)(%ar)%C(reset) %C(white)%s%C(reset) %C(dim white)- %an%C(reset)%C(bold yellow)%d%C(reset)' --all
        lg2 = log --author-date-order --graph --abbrev-commit --decorate --format=format:'%C(bold blue)%h%C(reset) - %C(bold cyan)%aD%C(reset) %C(bold green)(%ar)%C(reset)%C(bold yellow)%d%C(reset)%n''          %C(white)%s%C(reset) %C(dim white)- %an%C(reset)' --all
        lg = !"git lg1"
        df = diff
[pull]
	rebase = true
[rebase]
	autoStash = true
[credential]
	helper = cache
[color]
	branch = auto
	diff = auto
	status = auto
[color "branch"]
	current = yellow reverse
	local = yellow
	remote = green
[color "diff"]
	meta = yellow bold
	frag = magenta bold
	old = red bold
	new = green bold
[color "status"]
	added = green
	changed = red
	untracked = yellow
